{
  "name": "expresso-baileys",
  "version": "0.0.1",
  "description": "Rest API expresso with baileys",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prebuild": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist",
    "start": "nodemon --exec node ./dist/main.js",
    "dev": "npm-run-all build start",
    "dev:pm2": "npm-run-all build pm2",
    "pm2": "pm2-runtime ./dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "expressjs",
    "baileys",
    "typescript",
    "whatsapp-bot"
  ],
  "author": "masb0ymas <n.fajri@mail.com>",
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@whiskeysockets/baileys": "^6.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "expresso-core": "^0.7.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "pm2": "^5.3.0",
    "qrcode-terminal": "^0.12.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1"
  }
}
